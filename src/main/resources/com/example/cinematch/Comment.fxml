<?xml version="1.0" encoding="UTF-8"?>

<?import java.lang.*?>
<?import java.util.*?>
<?import javafx.scene.*?>
<?import javafx.scene.control.*?>
<?import javafx.scene.layout.*?>


<AnchorPane prefHeight="400.0" prefWidth="600.0"
            xmlns="http://javafx.com/javafx"
            xmlns:fx="http://javafx.com/fxml"
            fx:controller="com.example.cinematch.MovieDetailsController"> <Label layoutX="10.0" layoutY="14.0" style="-fx-font-weight: bold;" text="Users Comments" />

<TableView fx:id="commentsTable" AnchorPane.bottomAnchor="10.0" AnchorPane.leftAnchor="10.0" AnchorPane.rightAnchor="10.0" AnchorPane.topAnchor="50.0">
    <columns>

        <TableColumn fx:id="userColumn" prefWidth="100.0" text="User" />

        <TableColumn fx:id="textColumn" prefWidth="300.0" text="Comment" />

        <TableColumn fx:id="ratingColumn" prefWidth="80.0" text="Rating" />

        <TableColumn fx:id="dateColumn" prefWidth="100.0" text="Date" />

    </columns>
    <columnResizePolicy>
        <TableView fx:constant="CONSTRAINED_RESIZE_POLICY" />
    </columnResizePolicy>
</TableView>

</AnchorPane>